<!--- TEMPLATENAME: defaultfail.isml --->
<isdecorate template="checkout/pt_checkout">

	<isinclude template="util/modules"/>

	<h1><isprint value="${Resource.msg('altapay.defaultfail.header','altapay',null)}" encoding="off"/></h1>
	<isprint value="${Resource.msg('altapay.defaultfail.errormsg','altapay',null)}" encoding="off"/>
	<ul>
		<li><isprint value="${pdict.CurrentHttpParameterMap.error_message}" encoding="off"/></li>
	</ul>
	
	<table style="border: 1px solid gray; margin-top: 30px;">
		<thead>
			<tr>
				<th style="padding: 1em;"><isprint value="${Resource.msg('altapay.defaultfail.config.legend','altapay',null)}" encoding="off"/></th>
			</tr>
		</thead>
		<tbody>
			<tr>
				<td style="padding: 1em;"><isprint value="${Resource.msg('altapay.defaultfail.config.description','altapay',null)}" encoding="off"/></td>
			</tr>
		</tbody>
	</table>
	
</isdecorate>